package ex7;

public class Conta {
    String nomeTitular;
    int numeroConta;
    int numeroAgencia;
    float saldoConta;
    
    public Conta(String nomeT, int numC, int numA, float sal){
        nomeTitular = nomeT;
        numeroConta = numC;
        numeroAgencia = numA;
        saldoConta = sal;
    }
}
    


